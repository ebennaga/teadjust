<p id="notice"><%= notice %></p>

<h1>Tweets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <%= render 'form', tweet: @tweet %>
  
  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

